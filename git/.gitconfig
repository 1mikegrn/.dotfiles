[alias]
    C = "!f() { git add -A && git commit -m \"$1\"; }; f"
    I = "!f() { git init && git C \"initial commit\"; }; f"
    B = checkout -b
    pop = stash pop
    RESET = reset --hard HEAD
[init]
	defaultBranch = main
[user]
	email = self@michaelgreen.dev
	name = Michael Green
[pull]
	rebase = false

